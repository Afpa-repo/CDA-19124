<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.Tab?>
<?import javafx.scene.control.TabPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>


<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.afpa.stackorigami.HomeController">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="400.0" prefWidth="607.0">
         <children>
            <HBox prefHeight="60.0" prefWidth="607.0" style="-fx-background-color: gray;">
               <children>
                  <ImageView fitHeight="68.0" fitWidth="134.0" pickOnBounds="true" preserveRatio="true" />
                  <Label text="Dashboard Utilisateurs">
                     <font>
                        <Font size="33.0" />
                     </font>
                  </Label>
               </children>
            </HBox>
            <HBox prefHeight="285.0" prefWidth="607.0">
               <children>
                  <TabPane prefHeight="285.0" prefWidth="500.0" tabClosingPolicy="UNAVAILABLE">
                    <tabs>
                      <Tab text="Liste Clients">
                        <content>
                          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="257.0" prefWidth="607.0">
                                 <children>
                                    <TableView fx:id="table_user" prefHeight="256.0" prefWidth="494.0">
                                      <columns>
                                        <TableColumn fx:id="surname" prefWidth="123.0" text="Nom" />
                                        <TableColumn fx:id="first_name" prefWidth="123.0" text="Prénom" />
                                          <TableColumn fx:id="phone" prefWidth="125.0" text="Téléphone" />
                                          <TableColumn fx:id="type" prefWidth="122.0" text="Type" />
                                      </columns>
                                    </TableView>
                                 </children>
                              </AnchorPane>
                        </content>
                      </Tab>
                      <Tab text="Stats">
                        <content>
                          <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="256.0" prefWidth="464.0" />
                        </content>
                      </Tab>
                    </tabs>
                  </TabPane>
                  <VBox prefHeight="285.0" prefWidth="120.0" spacing="10.0">
                     <children>
                        <Button mnemonicParsing="false" onAction="#details" text="Détail" />
                        <Button mnemonicParsing="false" onAction="#ajoute" text="Ajout Client" />
                        <Button mnemonicParsing="false" onAction="#supprime" text="Suppression" />
                        <Button mnemonicParsing="false" onAction="#quitte" text="Quitter" />
                     </children>
                     <HBox.margin>
                        <Insets top="5.0" />
                     </HBox.margin>
                  </VBox>
               </children>
            </HBox>
            <Label text="StackOrigami" />
         </children>
      </VBox>
   </children>
</AnchorPane>
